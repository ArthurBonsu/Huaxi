{
  "name": "crosschain-passchain",
  "version": "1.0.0",
  "description": "Cross-chain transaction system with Passchain implementation",
  "main": "truffle-config.js",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "truffle test",
    "compile": "truffle compile",
    "migrate": "truffle migrate",
    "measure": "node scripts/measure_performance.js",
    "deploy:ethereum": "node scripts/deploy_ethereum.js",
    "deploy:polkadot": "node scripts/deploy_polkadot.js",
    "deploy:tatum": "node scripts/deploy_ethereum.js",
    "deploy:tatum:test": "truffle migrate --network tatum_testnet",
    "verify": "node scripts/verify-contracts.js",
    "analyze": "ANALYZE=true next build",
    "run:workflow": "python python/blockchainworkflow.py",
    "tsc-node": "tsc -p tsconfig.json",
    "lint:js": "eslint src --ext .ts --ext .tsx --fix",
    "check-updates": "ncu -u",
    "check-updates-minor": "ncu -u -t minor",
    "generate:changelog": "node ./scripts/changelog.js",
    "preinstall": "",
    "postinstall": "",
    "cz": "git cz",
    "clean": "rimraf ./cache && rimraf ./artifacts",
    "prettier": "prettier --write 'contracts/**/*.sol'",
    "test-clean": "yarn clean && yarn test",
    "list-files": "tree /F /A > project_structure.txt",
    "generate:route": "ts-node generateRoute.ts",
    "generate-structure": "tree -L 2 -I 'node_modules|.next|*.log|*.json|*.lock|public|static'"
  },
  "lint-staged": {
    "{**/*,*}.{json,css,md}": [
      "prettier --write"
    ],
    "{**/*,*}.{ts,tsx,js,jsx,sol}": [
      "eslint .eslintrc.js --fix --max-warnings=-1",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./fieryhartvim/scripts/commitizen"
    }
  },
  "extends": [
    "next",
    "prettier"
  ],
  "browserslist": [
    "defaults and supports es6-module",
    "maintained node versions"
  ],
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "https": false,
    "http": false,
    "crypto": false,
    "stream": false,
    "net": false,
    "tls": false,
    "constants": false,
    "zlib": false,
    "child_process": false,
    "console": false,
    "async_hooks": false,
    "ethereum-waffle": false,
    "stream_web": false,
    "util_types": false,
    "fs_promises": false,
    "querystring": false,
    "util": false,
    "fs-promises": false,
    "stream-web": false,
    "querystring-es3": false,
    "domain": false,
    "worker_threads": false,
    "vm": false
  },
